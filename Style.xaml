<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Baza">


    <Style x:Key="ButtonMargin" TargetType="Button">
        <Setter Property="Margin" Value="0, 2, 0, 0"/>
    </Style>
    <Style x:Key="ButtonMargin2" TargetType="Button">
        <Setter Property="Margin" Value="10, 2, 0, 0"/>
    </Style>
    <Style x:Key="StackPanelMargin" TargetType="StackPanel">
        <Setter Property="Margin" Value="10, 0, 0, 0"/>
    </Style>

</ResourceDictionary>